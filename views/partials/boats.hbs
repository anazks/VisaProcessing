<style>
    body {
    background-color: #eee
}

.card {
    border: none;
    border-radius: 10px
}

.c-details span {
    font-weight: 300;
    font-size: 13px
}

.icon {
    width: 50px;
    height: 50px;
    background-color: #eee;
    border-radius: 15px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 39px
}

.badge span {
    background-color: #fffbec;
    width: 60px;
    height: 25px;
    padding-bottom: 3px;
    border-radius: 5px;
    display: flex;
    color: #fed85d;
    justify-content: center;
    align-items: center
}

.progress {
    height: 10px;
    border-radius: 10px
}

.progress div {
    background-color: red
}

.text1 {
    font-size: 14px;
    font-weight: 600
}

.text2 {
    color: #a5aec0
}
.checkbtn{
    display: inline-block;
    margin: auto;
}
</style>
<body>
       <br><br><br><br><br><br><br>
    <div class="container mt-5 mb-3" style="m">
    <div class="row">
         {{#each jurneys}}
        <div class="col-md-4">
            <div class="card p-3 mb-2">
                <div class="d-flex justify-content-between">
                    <div class="d-flex flex-row align-items-center">
                        <div class=""> </div>
                        <div class="ms-2 c-details">
                            <h6 class="mb-0">{{Boatname}}</h6>  <span>1 days ago</span>
                        </div>
                    </div>
                    <div class="badge"> </div>
                </div>
                <div class="mt-5">
                    <p>{{from}} To {{to}}  <br>
                        Starting Date: {{Time}}/{{Date}}</p> 
                    <h3 class="heading"> <a href="/checkSeats/{{_id}}" class="btn btn-outline-primary checkbtn">Check Eligibility</a></h3>
                    <div class="mt-5">
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: 50%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                        {{!-- <div class="mt-3"> <span class="text1">32 Applied <span class="text2">of 50 capacity</span></span> </div> --}}
                    </div>
                </div>
            </div>
           
        </div>
        {{/each}}
    </div>
</div>
</body>
